{
  "compilerOptions": {
    "target": "es2024",
    "lib": ["ES2024", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "jsx": "react-native",
    "verbatimModuleSyntax": true,
    "types": ["mocha"],
    "sourceMap": true,
    "outDir": "../browser-tests-out",
    "strict": true,
    "skipLibCheck": true,
    "plugins": [
      {
        "transform": "../../dom-template-tsx/index.js",
        "componentFunctionName": "component",
        "componentModule": "../index.js"
      }
    ]
  },
  "include": ["./**/*.tsx", "./**/*.ts", "web-test-runner.config.mts"]
}
